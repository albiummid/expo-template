# Environment Configuration Template
# Copy this file to .env.development, .env.staging, or .env.production

# API Configuration (Required)
EXPO_PUBLIC_API_URL=https://api.example.com
EXPO_PUBLIC_API_VERSION=v1

# Environment (Required: development, staging, production)
EXPO_PUBLIC_ENVIRONMENT=development

# Feature Flags (Required: true/false)
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Third-party Services (Optional)
EXPO_PUBLIC_SENTRY_DSN=https://xxx@yyy.ingest.sentry.io/zzz
EXPO_PUBLIC_POSTHOG_API_KEY=phc_xxx

# App Configuration (Optional)
EXPO_PUBLIC_APP_NAME=Expo Template
EXPO_PUBLIC_SUPPORT_EMAIL=support@example.com
