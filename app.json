{
	"expo": {
		"name": "expo-template",
		"slug": "expo-template",
		"scheme": "acme",
		"version": "1.0.0",
		"orientation": "default",
		"userInterfaceStyle": "automatic",
		"splash": {
			"backgroundColor": "#000000",
			"resizeMode": "contain"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.yourcompany.expotemplate",
			"infoPlist": {
				"UIBackgroundModes": ["remote-notification"]
			}
		},
		"android": {
			"package": "com.yourcompany.expotemplate",
			"adaptiveIcon": {
				"backgroundColor": "#000000"
			},
			"permissions": [
				"android.permission.INTERNET",
				"android.permission.ACCESS_NETWORK_STATE"
			]
		},
		"web": {
			"output": "static"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#000000"
				}
			],
			[
				"expo-notifications",
				{
					"icon": "./assets/notification-icon.png",
					"color": "#000000"
				}
			]
		],
		"experiments": {
			"tsconfigPaths": true
		}
	}
}
